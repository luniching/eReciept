<?xml version="1.0" encoding="UTF-8"?>
<ContentBundle
	instanceOf="login"
	version="2.0"
	xmlns="http://www.ebay.com/2006/v4/content"
	xmlns:its="http://www.w3.org/2005/11/its"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.ebay.com/2006/v4/content ../../../schema/V4Content2.xsd">

	<ContentElement id="email">Email</ContentElement>
	<ContentElement id="password">Password</ContentElement>
	<ContentElement id="login">Log In</ContentElement>

	<ContentList id="error">
		<ContentElement id="invalidEmail">Please enter a valid email address.</ContentElement>
		<ContentElement id="invalidCredentials">Could not login. Invalid credentials provided.</ContentElement>
	</ContentList>

	<ContentElement id="welcome">Welcome back, <DPH id="fullName"/>!</ContentElement>

	<ContentMap id="feedbackDetails">
		<ContentElement id="NONE">You have <DPH id="score"/> feedback scores.</ContentElement>
		<ContentElement id="ONE">You have <DPH id="score"/> feedback score.</ContentElement>
		<ContentElement id="MANY">You have <DPH id="score"/> feedback scores.</ContentElement>
	</ContentMap>

</ContentBundle>